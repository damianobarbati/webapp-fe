{
  "headers": [
    {
      "source": "index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source" : "**/*.@(jpg|jpeg|gif|png|woff2|css|js|map)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, immutable"
        }
      ]
    }
  ]
}
