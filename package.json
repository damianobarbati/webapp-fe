{
  "name": "webapp-fe",
  "version": "1.0.0",
  "description": "Front-end starter kit for web applications",
  "license": "UNLICENSED",
  "author": "Damiano Barbati <damiano.barbati@gmail.com> (http://github.com/damianobarbati)",
  "repository": "https://github.com/damianobarbati/webapp-fe",
  "type": "module",
  "scripts": {
    "eslint": "eslint --ignore-path .gitignore --fix",
    "test": "NODE_ENV=development DEBUG=pw:webserver playwright test --headed",
    "check": "tsc --noEmit",
    "prebuild": "rm -rf dist",
    "build": "PARCEL_BUNDLE_ANALYZER=1 yarn build:${NODE_ENV:-development}",
    "build:development": "NODE_ENV=development parcel src/index.html --no-cache -p 80",
    "build:staging": "NODE_ENV=staging parcel build src/index.html --no-cache --detailed-report=1000",
    "build:production": "NODE_ENV=production parcel build src/index.html --no-cache --detailed-report=1000",
    "serve": "npx serve@13 -p 80 -C -s ./dist"
  },
  "multipleStaticFileCopier": [
    {
      "origin": "assets",
      "destination": "dist/assets"
    }
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "jwt-decode": "^3.1.2",
    "jwt-encode": "^1.0.1",
    "lodash-es": "^4.17.21",
    "miragejs": "^0.1.43",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.30.0",
    "react-jss": "^10.9.0",
    "react-query": "^3.38.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.9",
    "tailwindcss": "^3.0.24",
    "typeface-muli": "^1.1.13"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.5.0",
    "@playwright/test": "^1.21.1",
    "@testing-library/react": "^13.1.1",
    "axios-logger": "^2.6.0",
    "eslint-config-xs": "^1.1.3",
    "lighthouse": "^9.5.0",
    "parcel": "^2.5.0",
    "parcel-reporter-multiple-static-file-copier": "^1.0.5",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "process": "^0.11.10",
    "sharp": "0.29.3",
    "tsc": "^2.0.4"
  }
}
