{
    "name": "webapp-fe",
    "version": "1.0.0",
    "description": "Front-end starter kit for web applications",
    "license": "UNLICENSED",
    "author": "Damiano Barbati <damiano.barbati@gmail.com> (http://github.com/damianobarbati)",
    "repository": "https://github.com/damianobarbati/webapp-fe",
    "type": "module",
    "scripts": {
        "eslint": "eslint --ignore-path .gitignore",
        "build": "yarn build:${NODE_ENV:-development}",
        "build:development": "parcel src/index.html --open",
        "build:staging": "parcel build src/index.html --no-cache --no-autoinstall --no-source-maps && gzip -rk dist/*",
        "build:production": "parcel build src/index.html --no-cache --no-autoinstall --no-source-maps && gzip -rk dist/*"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "classnames": "^2.2.5",
        "jss-reset": "^1.1.3",
        "jwt-decode": "^2.2.0",
        "lodash-es": "^4.17.15",
        "preact": "^10.4.8",
        "react-hook-form": "^6.7.0",
        "react-jss": "^10.4.0",
        "react-router-dom": "^5.2.0",
        "recharts": "^1.8.5",
        "typeface-muli": "^1.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/eslint-parser": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "core-js": "^3.6.5",
        "eslint": "^7.7.0",
        "eslint-plugin-react": "^7.20.6",
        "parcel": "1.12.4",
        "parcel-plugin-bundle-visualiser": "^1.2.0",
        "parcel-plugin-notifier": "^1.0.0",
        "parcel-plugin-nuke-dist": "^1.0.1"
    },
    "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
    }
}
