{
    "name": "webapp-fe",
    "version": "1.0.0",
    "description": "Front-end starter kit for web applications",
    "license": "UNLICENSED",
    "author": "Damiano Barbati <damiano.barbati@gmail.com> (http://github.com/damianobarbati)",
    "repository": "https://github.com/damianobarbati/webapp-fe",
    "type": "module",
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "eslint": "eslint --ignore-path .gitignore",
        "build": "rm -rf dist; yarn build:${NODE_ENV:-development}",
        "build:development": "parcel src/index.html --open --no-cache",
        "build:staging": "PARCEL_BUNDLE_ANALYZER=1 parcel build src/index.html --no-cache",
        "build:production": "PARCEL_BUNDLE_ANALYZER=1 parcel build src/index.html --no-cache",
        "serve": "npx http-server ./dist --port 80 --gzip --cors -o --proxy http://localhost?"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "classnames": "^2.2.5",
        "jss-reset": "^1.1.3",
        "jwt-decode": "^3.1.2",
        "lodash-es": "^4.17.20",
        "preact": "^10.5.12",
        "react-hook-form": "^6.15.1",
        "react-jss": "^10.5.1",
        "react-router-dom": "^5.2.0",
        "recharts": "^2.0.6",
        "typeface-muli": "^1.1.13"
    },
    "devDependencies": {
        "@babel/core": "^7.12.13",
        "@babel/eslint-parser": "^7.12.13",
        "@babel/preset-env": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "core-js": "^3.8.3",
        "eslint": "^7.19.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^26.6.3",
        "parcel": "2.0.0-beta.1",
        "parcel-plugin-notifier": "^1.0.0",
        "parcel-plugin-nuke-dist": "^1.0.1"
    },
    "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
    }
}
